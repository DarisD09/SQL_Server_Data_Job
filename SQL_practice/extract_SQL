-- January
CREATE TABLE IF NOT EXISTS january_job AS
SELECT 
    *
FROM 
    job_postings_fact
WHERE
    EXTRACT(MONTH FROM Job_posted_date) = 1;

-- February
CREATE TABLE IF NOT EXISTS february_job AS
SELECT 
    *
FROM 
    job_postings_fact
WHERE
    EXTRACT(MONTH FROM Job_posted_date) = 2;

-- March
CREATE TABLE IF NOT EXISTS march_job AS
SELECT 
    *
FROM 
    job_postings_fact
WHERE
    EXTRACT(MONTH FROM Job_posted_date) = 3;


SELECT
    Job_posted_date:: DATE
FROM
    march_job